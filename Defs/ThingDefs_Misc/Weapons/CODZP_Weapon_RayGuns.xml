<!--Ray gun-->
<ThingDef ParentName="CODZP_BaseWonderWeaponGun">
	<defName>CodZP_Gun_RayGun</defName>
	<label>RayGun</label>
	<description>A high tech that shoots green explosive plasma!</description>
	<techLevel>Spacer</techLevel>
	<graphicData>
		<texPath>Things/Item/Equipment/WeaponRanged/Rays/CodZP_RayGun</texPath>
		<graphicClass>Graphic_Single</graphicClass>
	</graphicData>
	<soundInteract>CodZP_Interact_Raygun</soundInteract>
	<statBases>
		<Mass>2.3</Mass>
		<AccuracyTouch>0.87</AccuracyTouch>
		<AccuracyShort>0.71</AccuracyShort>
		<AccuracyMedium>0.65</AccuracyMedium>
		<AccuracyLong>0.45</AccuracyLong>
		<RangedWeapon_Cooldown>1.21</RangedWeapon_Cooldown>
	</statBases>
	<costList>
		<Uranium>10</Uranium>
		<Plasteel>100</Plasteel>
		<ComponentSpacer>1</ComponentSpacer>
	</costList>
	<weaponTags>
		<li>SpacerGun</li>
	</weaponTags>
	<verbs>
		<li>
			<verbClass>Verb_Shoot</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>CodZP_Bullet_Raygun</defaultProjectile>
			<warmupTime>1.34</warmupTime>
			<forcedMissRadius>0.5</forcedMissRadius>
			<range>23.9</range>
			<soundCast>CodZP_Shot_Raygun</soundCast>
			<soundCastTail>GunTail_Medium</soundCastTail>
			<muzzleFlashScale>9</muzzleFlashScale>
		</li>
	</verbs>
	<tickerType>Normal</tickerType>
	<tools>
		<li>
			<label>grip</label>
			<capacities>
				<li>Blunt</li>
			</capacities>
			<power>9</power>
			<cooldownTime>2</cooldownTime>
		</li>
		<li>
			<label>barrel</label>
			<capacities>
				<li>Blunt</li>
				<li>Poke</li>
			</capacities>
			<power>9</power>
			<cooldownTime>2</cooldownTime>
		</li>
	</tools>
</ThingDef>
